<div class="wait-content">
    <table class="table table-hover">
        <thead class="table-thead">
            <th>
                <input type="checkbox" name="allbox">
                客户信息
            </th>
            <th>消息时间</th>
            <th>上次接待客服</th>
            <th>操作</th>
        </thead>
         <% for(var i=0; i<messages.length; i++) { %>
            <tbody>
                <tr style="height:60px">
                <td style="position:relative;">
                    <input type="checkbox" name="allbox">
                    <img src=<%=messages[i].headImage%> class="user-photo">
                    <span class="top-text user-name"><%=messages[i].nikename%></span>
                    <span class="bottom-text massage-contnet">消息:<%=messages[i].content%></span>
                </td>
                <td style="position:relative;">
                    <span class="top-text"><%=messages[i].creattime%></span>
                    <span class="bottom-text">48小时后过期</span>
                </td>
                <td style="position:relative;">
                    <span class="top-text">李强</span>
                    <span class="bottom-text">2017-6-13 16:15:32</span>
                </td>
                <td style="position:relative;">
                    <span class="top-text btn-access" >接入</span>
                </td>
             </tr>
            </tbody>
         <% } %>
    </table>
</div>