<div class="chatheader">
  <span class="chat_header_name"><%=nickename%></span>
  <div class="chat_header_icons">
    <span class="chat_header_icon transfer"></span>
    <span class="chat_header_icon exit" "></span>
  </div>
</div>
<div class="scroll-wrapper " id='chatScroll'>
  <div class="scroll-content ">
  <% for(var i=0; i<messages.length; i++) { %>
  
    <div class="message_box message_box <%=messages[i].from_self%> ">
      <div class="message_box_time "><%=messages[i].creattime%></div>
      <img src=<%=messages[i].headImageUrl%> class="mssage_box_pic " style="display:inline-block; ">
      <div class="message_content ">
        <p class="message_content_inner "><%=messages[i].messageContent%></p>
      </div>
   
  </div>
 <% } %>
  </div>
</div>
<div class="chat_edit ">
<!-- <div class="chat_edit_items ">表情</div> -->
<!-- <div class="chat_edit_input "> -->
<textarea id='enterKey' class="edit_area "></textarea>
<!-- </div> -->
<div class="chat_edit_footer ">按Enter发送</div>
</div>